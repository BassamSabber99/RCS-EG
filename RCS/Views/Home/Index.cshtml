@{
    ViewBag.Title = "RCS";
}

 <!--Slider-->
<div id="carouselExampleCaptions" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
        <li data-target="#carouselExampleCaptions" data-slide-to="0" class="active"></li>
        <li data-target="#carouselExampleCaptions" data-slide-to="1"></li>
        <li data-target="#carouselExampleCaptions" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="~/images/1.jpg" class="d-block w-100" alt="...">

        </div>
        <div class="carousel-item">
            <img src="~/images/2.jpg" class="d-block w-100" alt="...">

        </div>
        <div class="carousel-item">
            <img src="~/images/3.jpg" class="d-block w-100" alt="...">

        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleCaptions" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleCaptions" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
<!--END-Slider-->
  <!--Section 1-->
<div class="Branding col-md-12">
    <video autoplay muted loop id="myVideo">
        <source src="~/images/Cooking.mp4" type="video/mp4">
    </video>
    <div class="text container">
        <h1 class="text-center  wow fadeIn slow"><span class="text-warning">R</span>CS</h1>
        <div class="row justify-content-center">
            <div class="col-md-12 wow fadeIn slow">
                <p class="@RCS.Resources.Resource.text" style="font-size: 25px;">
                    @RCS.Resources.Resource.content1
                </p>
            </div>
        </div>
        <div class="row justify-content-center mt-4">
            <div class="blockImg col-md-4">
                <p class="text-center text-warning font-weight-bold wow fadeIn slow">@RCS.Resources.Resource.special</p>
                <div class="imgBox">
                    <img src="~/images/thinking.jpg" class="wow bounceIn slow">
                </div>
            </div>
            <div class="blockImg col-md-4">
                <p class="text-center text-warning font-weight-bold wow fadeIn slow">@RCS.Resources.Resource.creativity</p>
                <div class="imgBox">
                    <img src="~/images/creativity.png" class="wow bounceIn slow" width="100%">
                </div>
            </div>
            <div class="blockImg col-md-4">
                <p class="text-center text-warning font-weight-bold wow fadeIn slow">@RCS.Resources.Resource.individ</p>
                <div class="imgBox">
                    <img src="~/images/Motivation.jpg" class="wow bounceIn slow" width="100%">
                </div>
            </div>
        </div>
    </div>
</div>
<!--END-Section 1-->
<!--Hero Text 1-->
<div class="sec2 text-light p-4">
    <h2 class="text-center mt-4 wow pulse slow">@RCS.Resources.Resource.SpecialWork <span class="text-primary"><i class="fas fa-smile-wink"></i></span></h2>
    <p class="text-center container wow fadeIn slow">
       @RCS.Resources.Resource.content2
    </p>
</div>
<!--END-Hero Text 1-->
<!--Section 3-->
<div class="sec3">
    <div class="container-fluid p-2">
        <div class="row">
            <div class="imgsec3 col-md-6 p-0">
                <img src="~/images/sec3.jpg" class="w-100" alt="">
            </div>
            <div class="overlay col-md-6 p-0 bounce wow slideOutUp slow  animated">
            </div>
            <div class="text2 col-md-6 p-4">
                <div class="container mt-5 text-white">
                    <p>@RCS.Resources.Resource.parag</p>
                    <ul>
                        <li class="@RCS.Resources.Resource.text">@RCS.Resources.Resource.l1</li>
                        <li class="@RCS.Resources.Resource.text">@RCS.Resources.Resource.l2</li>
                        <li class="@RCS.Resources.Resource.text">@RCS.Resources.Resource.l3</li>
                    </ul>
                </div>
                <div class="container mt-5">
                    <div class="row justify-content-center">
                        <div class="col-md-6 mb-1">
                            <h4 class="text-center text-white mb-2 wow fadeIn">@RCS.Resources.Resource.RestaurantsProcessing</h4>
                            <img src="~/images/img1sec3.jpg" class="w-100 wow bounceInUp">
                        </div>
                        <div class="col-md-6 mb-1">
                            <h4 class="text-center text-white mb-2 wow fadeIn">@RCS.Resources.Resource.CafeProcessing</h4>
                            <img src="~/images/img2sec3.jpg" class="w-100 wow bounceInUp">
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
<!--END-Section 3-->
<!--Section 4-->
<div class="sec4 overflow-hidden">
    <div class="container-fluid p-2 overflow-hidden">
        <div class="row overflow-hidden">
            <div class="text2 col-md-6 p-4 overflow-hidden">
                <div class="container mt-5 text-white">
                    <h2 class="text-center mt-5 wow pulse slow">@RCS.Resources.Resource.MakeAnAdvertise <span class="text-warning"><i class="fas fa-smile-wink"></i></span></h2>
                    <p class="text-center container wow fadeIn slow font-weight-bold">@RCS.Resources.Resource.content3</p>
                    <div class="container d-flex justify-content-center">
                        <a href="~/Advertises/index" class="btn btn-lg active" role="button" aria-pressed="true">@RCS.Resources.Resource.go <i class="fas fa-arrow-right"></i></a>
                    </div>
                </div>
            </div>
            <div class="imgsec3 col-md-6 p-0">
                <img src="~/images/img1sec4.jpg" class="w-100" alt="">
            </div>
            <div class="overlay col-md-6 p-0 bounce wow slideOutUp slow  animated ">
            </div>
        </div>
    </div>
</div>
<!--END-Section 4-->


<section class="AboutUs">
    <!--About US-->
    <div class="container mt-4 text-white">
        <h1 class="text-center">@RCS.Resources.Resource.AboutUs</h1>
        <p class="wow bounceInUp font-weight-bold @RCS.Resources.Resource.text">
            @RCS.Resources.Resource.abContent
        </p>
    </div>
    <!--END-About US-->
    <!--Who Are We-->
    <div class="container-fluid p-4 mt-4 text-white" style="background-color: #fa7d09;">
        <h1 class="container text-center">@RCS.Resources.Resource.WhoWeAre</h1>
        <p class="container wow bounceInUp font-weight-bold @RCS.Resources.Resource.text">@RCS.Resources.Resource.whCon</p>
    </div>
    <!--END-Who Are We-->
    <!--Contact US-->
    <div class="container-fluid p-4 overflow-hidden text-white" style="background-color: #ff4301;">
        <h1 class="text-center">@RCS.Resources.Resource.ContactUs</h1>
        <div class="container p-4">
            <div class="row">
                <div class="col-md-6">
                    <h6 class="mt-5 wow bounceInUp @RCS.Resources.Resource.text">@RCS.Resources.Resource.address</h6>
                    <h6 class="wow bounceInUp @RCS.Resources.Resource.text ">@RCS.Resources.Resource.mob</h6>
                    <h6 class="wow bounceInUp @RCS.Resources.Resource.text">@RCS.Resources.Resource.lnd</h6>
                    <h6 class="wow bounceInUp @RCS.Resources.Resource.text">@RCS.Resources.Resource.email</h6>
                </div>
                <div class="col-md-6 wow fadeIn">
                    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3452.2485432407225!2d31.31175161545602!3d30.087067423512117!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14583e2ddedf1f45%3A0xcd14efb03db7be!2s22%20Manshiet%20Al%20Tairan%2C%20Mansheya%20El-Bakry%2C%20Heliopolis%2C%20Cairo%20Governorate%2C%20Egypt!5e0!3m2!1sen!2sqa!4v1600249171693!5m2!1sen!2sqa"
                            width="600" height="450" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
                </div>
            </div>
        </div>
    </div>
    <!--END-Contact US-->
</section>


@section scripts{

    <script>
        new WOW().init();
        $(document).ready(function () {
            $("nav .navbar-collapse ul li a:eq(0)").css("color", "#ff4301")
        })
    </script>  
   
}